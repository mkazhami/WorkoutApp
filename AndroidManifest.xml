<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workout"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.workout.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.workout.CustomizeWorkout"
            android:label="@string/title_activity_customize_workout" >
        </activity>
        <activity
            android:name="com.example.workout.UseWorkout"
            android:label="@string/title_activity_use_workout">
        </activity>
        <activity
            android:name="com.example.workout.AddExercise"
            android:label="@string/title_activity_customize_exercise" >
        </activity>
        <activity 
            android:name="com.example.workout.ViewHistoryMenu"
            android:label="@string/title_activity_view_history_menu">
        </activity>
        <activity 
            android:name="com.example.workout.ViewHistoryExercise"
            android:label="@string/title_activity_view_history_exercise">
        </activity>
        <activity
            android:name="org.achartengine.GraphicalActivity">
        </activity>
    </application>

</manifest>
